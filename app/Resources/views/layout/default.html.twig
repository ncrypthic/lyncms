{# empty Twig template #}
{% extends "::base.html.twig" %}

{% block container %}
<div class="container">
    <div>
        <nav>
            <div class="navbar navbar-default">
                {{ knp_menu_render('navigation', { 'rootClass': 'nav navbar-nav' } ) }}
            </div>
        </nav>
    </div>
    <div class="col-md-9">{% block main %}{% endblock main %}</div>
    <div class="col-md-3">:articles(1, "test"):{% block sidebar %}{% endblock sidebar %}</div>
    <div>{% block footer %}{% endblock footer %}</div>
</div>
{% endblock container %}

{% block root %}
    {% set listAttributes = item.childrenAttributes|merge({ 'class': [
    options.rootClass is defined ? options.rootClass : '' ] }) %}
    {{ block('list') -}}
{% endblock %}
